n=int(input('enter no.of rows'))
m=int(input('enter no.of columns'))
N=[]
for i in range(0,n):
      M=[]
      print('enter elements in ',i+1,'row')
      for t in range(0,m):
            x=int(input())
            M.append(x)
      N.append(M)
b=[]
for i in N:
      g=0
      for e in i:
            g+=e
      b.append(g)
if len(list(set(b)))==1:
      print('ROW MAGIC')
else:
      print('NOT A ROW MAGIC')
