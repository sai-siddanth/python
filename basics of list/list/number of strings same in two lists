n=int(input('enter length of 1st list'))
m=int(input('enter length of 2st list'))
N=[]
M=[]
T_F=0
print('enter in 1st list')
for i in range(0,n):
      x=input()
      N.append(x)
print('enter in 2nd list')
for i in range(0,m):
      x=input()
      M.append(x)
for i in N:
    for a in M:
        if i==a:
            T_F+=1
if T_F!=0:
    print('no.of strings are same = ',T_F)
else:
    print('no strings are same')
